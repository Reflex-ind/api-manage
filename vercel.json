{
  "rewrites": [
    {
      "source": "/ban/:path*",
      "destination": "https://nationbancheck.vercel.app/:path*"
    },
    {
      "source": "/info/:path*",
      "destination": "https://nation-info-api.vercel.app/:path*"
    },
    {
      "source": "/like/:path*",
      "destination": "https://nation-like-api.vercel.app/:path*"
    },
    {
      "source": "/token/:path*",
      "destination": "https://nation-jwt-api.vercel.app/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}

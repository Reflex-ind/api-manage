{
  "rewrites": [
    {
      "source": "/ban",
      "destination": "https://nationbancheck.vercel.app/ban"
    },
    {
      "source": "/info",
      "destination": "https://nation-info-api.vercel.app/info"
    },
    {
      "source": "/like",
      "destination": "https://nation-like-api.vercel.app/like"
    },
    {
      "source": "/token",
      "destination": "https://nation-jwt-api.vercel.app/token"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
